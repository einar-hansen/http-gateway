{
    "name": "einar-hansen/http-gateway",
    "description": "A PHP service that allows you to communicate with external apis using PSR-18 clients.",
    "keywords": [
        "einar",
        "einar-hansen",
        "laravel",
        "http",
        "psr-18"
    ],
    "homepage": "https://github.com/einar-hansen/http-gateway",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Einar Hansen",
            "email": "einar@einarhansen.dev",
            "homepage": "https://einarhansen.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "php-http/discovery": "^1.14",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "EinarHansen\\Http\\": "src/"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan --level=5 analyse",
        "format": "vendor/bin/pint",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
